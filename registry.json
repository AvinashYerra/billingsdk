{
    "$schema": "https://ui.shadcn.com/schema.json",
    "name": "billingsdk",
    "homepage": "https://billingsdk.com",
    "items": [
        {
            "name": "index",
            "type": "registry:lib",
            "title": "BillingSDK Constants",
            "description": "Constants and types for BillingSDK components",
            "files": [
                {
                    "path": "src/registry/lib/const.ts",
                    "type": "registry:lib",
                    "target": "src/lib/const.ts"
                }
            ]
        },
        {
            "name": "hello-world",
            "type": "registry:block",
            "title": "Hello World Component",
            "description": "A simple hello world component",
            "files": [
                {
                    "path": "src/registry/billingsdk/hello-world.tsx",
                    "type": "registry:component",
                    "target": "components/billingsdk/hello-world.tsx"
                }
            ]
        },
        {
            "name": "minimal-pricing-table",
            "type": "registry:block",
            "title": "Minimal Pricing Table",
            "description": "A minimal pricing table component",
            "files": [
                {
                    "path": "src/registry/billingsdk/minimal-pricing-table.tsx",
                    "type": "registry:component",
                    "target": "components/billingsdk/minimal-pricing-table.tsx"
                },
                {
                    "path": "src/registry/billingsdk/demo/minimal-pricing-table-demo.tsx",
                    "type": "registry:component",
                    "target": "components/minimal-pricing-table-demo.tsx"
                }
            ],
            "dependencies": [
                "lucide-react"
            ],
            "registryDependencies": [
                "button",
                "card",
                "badge",
                "switch",
                "utils"
